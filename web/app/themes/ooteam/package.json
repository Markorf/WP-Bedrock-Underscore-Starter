  "devDependencies": {
    "@wordpress/eslint-plugin": "^7.2.1",
    "@wordpress/scripts": "^12.1.0",
    "browser-sync": "^2.26.12",
    "browser-sync-webpack-plugin": "^2.0.1",
    "cross-env": "^7.0.2",
    "dir-archiver": "^1.1.1",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
